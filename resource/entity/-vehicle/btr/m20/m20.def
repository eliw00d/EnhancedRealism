{game_entity
	(include "/properties/armoredcar.ext")
	(include "/properties/tank_crew.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.0))
	{props "vehicle" "armored_car" "tank" "turret" "wheeled" "with_gun" "vision_vehicle_low"}

	{targetclass "armoredcar" "ap bullet"}
	(include "/properties/tank_crew.ext")
	{extender "enumerator"}
	
	{volume "body"
		{tags "body"}
		{component "body"}
		{thickness 10
			{front	16}
		}
	}
	{volume "body1"
		{tags "body"}
		{component "body"}
		{thickness 10
			{front	16}
		}
	}
	{volume "body2"
		{tags "body"}
		{component "body"}
		{thickness 10
			{front	16}
		}
	}
	{volume "turret"
		{component "turret"}
		{tags "turret"}
		{thickness 19}
	}
	{volume "mantlet"
		{component "turret"}
		{tags "turret"}
		{thickness 25}
	}
	{volume "crew"
		{able {contact_ground 0}}
	}
	{component "turret"
		{RepairTime 20}
	}

	{volume "engine"
		{thickness 10
			{rear	15}
		}
	}
	{Volume "Gun"
		{thickness 10}
	}
	{component "Gun"
		{RepairTime 20}
	}
		
	("armor_medium" args 1.2)
	
	("shield" args 8)
	{extender "shield"
		{on}
	}

	{component "shieldl1" {tags "shield"}}
	{component "shieldl2" {tags "shield"}}
	{component "shieldr1" {tags "shield"}}
	{component "shieldr2" {tags "shield"}}
	
	{boarder
		{anm "left"
			{forward	{begin	"board_tank_left"} {end "seat_gunner_stand"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"} {end "seat_gunner_stand"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
	}

	("crew_2_human")
	("crew_human_visible" place(gunner)	door1(emit1) door2(emit2) visor(main))

	{extender "inventory"
		{weapon "mgun"
			{mask "weapon mgun"}
		}
		{box
			{item "browning" filled {user "mgun"}}
			{item "ammo hmgun" 1000}
			{item "weapon bazooka" 1}			
			{item "ammo bazooka" 8}
		}
	}
	{Weaponry
		{place "mgun"
			{type "mgun_main"}
			{LinkBone "mgun2"}
			{LinkAnimation "off"}
			{charger "gunner"}
			("abm_mgun_heavy")
		}
	}
	{sensor
		{visor "main"
			{bone "mgun2"}
		}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")

	{mass 7000}
	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	42}
	  		{maxSpeedAtMaxTurn 15}	
			{StartTime	7}
			{BrakeTime	1.5}	
			{StopTime	2}
			{TurnRadius	6}
		}
		{FuelBag
			{volume 215 }	
			{rate	30 }   
			{remain 215 }
			{fuel "fuel"}
		}
		{speed
        	{Normal  12}
        	{Slow  20}
    	}
		("car_trace" fx("tracewheel01") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel01") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL3") boner("FXtraceR3"))
	}

	{Extension "m20.mdl"}

}

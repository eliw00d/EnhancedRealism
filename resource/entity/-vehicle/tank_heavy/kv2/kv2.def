{game_entity
	(include "/properties/tank.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{collider "tank.heavy"}
	{props "heavy" "turret_rotator" "vision_vehicle_maximum" "muzzle_maximum"}
	
	{Volume "body1"
		{tags "body"}
		{component "body"}
	}
	{Volume "body2"
		{tags "body2"}
		{component "body"}
	}
	{Volume "body3"
		{tags "body3"}
		{component "body"}
	}
	{Volume "turret1"
		{tags "turret"}
		{component "turret"}
	}
	
	{Volume "engine"
		{tags "engine"}
		{component "engine"}
	}

	("armor_heavy" turret(75 75 75 35) body(75 75 70 30) body2(40 75 70 30) body3(75 75 70 30))
	("armor_engine"  t(30 75 30 30))		
	("armor_mantlet" t(110))

	{extender "enumerator"}

	("crew_4_human")
	{placer 
		{place "commander" {visor "back"}}
	}
	{boarder
		{anm "left"
			{forward	{begin	"board_Kv-85"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_Kv-85"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_HighArmor"} 	{end "seat_armor"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_HighArmor"} {end "seat_armor"}}
			{reverse 	{end	"emit_body"}}
		}
	}

	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit4)  animation(armor))
	("seat_human" place(seat3) bone(seat03) door(emit5)  animation(armor))
	("seat_human" place(seat4) bone(seat04) door(emit6)  animation(armor1))
	("seat_human" place(seat5) bone(seat05) door(emit7)  animation(armor))
	("seat_human" place(seat6) bone(seat06) door(emit8)  animation(armor1))
	("seat_human" place(seat7) bone(seat07) door(emit9)  animation(armor))
	("seat_human" place(seat8) bone(seat08) door(emit10)  animation(armor1))
	("seat_human" place(seat9) bone(seat09) door(emit11)  animation(armor))
	
	{sensor
		{visor "back"
			{vision "tank_back"}
			{bone "foresight4"}
		}
	}

	{extender "inventory"
		{box
            {item "ammo bullet152 fg" 36}
            {item  "ammo mgun" 2150}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "152mm_m10t" filling "ammo bullet152 fg" 1}		
			("abm_medium")
		}
		{place "mgun3"
			{type "mgun_hull"}
			{weapon "dt" filled}
			{foresight "foresight6"}
			{gunner "driver"}
			{charger "driver"}
			("abm_mgun")
			{basic}
		}
		{place "mgun1"
			{type "mgun_rear"}
			{weapon "dt" filled}
			{foresight "foresight4"}
			{gunner "commander"}
			{charger "commander"}
			("abm_mgun")
		}

	}
	{mass 52000}
	{Chassis
		("tank_trace" fx("tracks_big") step(0.47) len(0.183))
		{Locomotion ("locomotion")
			{MaxSpeed	20}
			{maxSpeedAtMaxTurn 8}
			{StartTime	18}
			{TurnTime	15}
			{TurnStart  2} 
		{BrakeTime  2}
		{StopTime   3}
		{TurnRadius 10}
		{TurnStop   1}
		{Gears      0.1 0.5 1.0}
		{RearGears  0.1 0.48} 
		}
		{FuelBag
			{volume 600 }	
			{rate	150 }   
			{remain 600 }
			{fuel "fuel"}
		}
		{speed
        	{Normal  10}
        	{Slow  12}
    	}
	}
	
	{Extension "kv2.mdl"}

}

			{weapon "mg34" filled}
			{foresight "foresight4"}
			{gunner "driver2"}
			{charger "driver2"}
			("abm_mgun")
			{basic}
		}
	}

	{mass 22000}
	{Chassis
		("tank_trace" fx("tracks_mid") step(0.52) len(0.7))
